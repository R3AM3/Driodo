<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="50dp"/>
            <gradient android:angle="45" android:startColor="@color/colorPrimary" android:endColor="@color/colorPrimaryDark"/>
        </shape>
    </item>
<item >
    <shape android:shape="rectangle">
        <corners android:radius="50dp"/>
        <gradient android:angle="45" android:startColor="#7ef7aa" android:endColor="#08c0ec"/>
    </shape>
</item>
</selector>